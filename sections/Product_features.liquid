<script>
  document.addEventListener('DOMContentLoaded', function () {
    // Handle accordion icon toggling on initial page load
    document.querySelectorAll('.accordion-item').forEach((accordionItem) => {
      const chevronUp = accordionItem.querySelector('.chevron-up');
      const chevronDown = accordionItem.querySelector('.chevron-down');

      // If the accordion item is active (open by default), show Chevron Up and hide Chevron Down
      if (accordionItem.classList.contains('active')) {
        chevronUp.style.display = 'block';
        chevronDown.style.display = 'none';
      } else {
        chevronUp.style.display = 'none';
        chevronDown.style.display = 'block';
      }
    });
  });

  document.querySelectorAll('.accordion-header').forEach((button) => {
    button.addEventListener('click', function () {
      const accordionItem = button.parentElement;
      const chevronUp = button.querySelector('.chevron-up');
      const chevronDown = button.querySelector('.chevron-down');

      // Toggle the current accordion item's active state
      if (accordionItem.classList.contains('active')) {
        accordionItem.classList.remove('active');
        // Show Chevron Down (closed state), hide Chevron Up
        chevronUp.style.display = 'none';
        chevronDown.style.display = 'block';
      } else {
        document.querySelectorAll('.accordion-item').forEach((item) => {
          item.classList.remove('active');
          item.querySelector('.chevron-up').style.display = 'none';
          item.querySelector('.chevron-down').style.display = 'block';
        });
        // Open the clicked accordion item
        setTimeout(() => {
          accordionItem.classList.add('active');
          // Show Chevron Up (open state), hide Chevron Down
          chevronUp.style.display = 'block';
          chevronDown.style.display = 'none';
        }, 300);
      }
    });
  });
</script>
