<style>
  .accordion {
    display: block !important;
  }

  .accordion-item {
    border-top: 2px solid #000000;
  }

  .accordion-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 15px 10px;
    cursor: pointer;
    font-weight: bold;
    font-size: 1em;
    width: 100%;
    border: none;
    outline: none;
    text-align: left;
    color: currentColor;
  }

  .accordion-header-content {
    display: flex;
    align-items: center;
  }

  .accordion-header-content img.accordion-icon-img {
    margin-right: 10px;
    width: 30px;
    height: 30px;
  }

  .accordion-header h3 {
    margin: 0;
    text-align: left;
    flex-grow: 1;
  }

  .accordion-icon {
    font-size: 1.2em;
    transition: transform 0.3s ease;
  }

  .accordion-content {
    display: flex;
    justify-content: space-between;
    gap: var(--gutter);
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease;
  }

  .div_p {
    border: 1px solid #000000;
    padding: var(--gutter);
    flex: 1;
  }

  .accordion-item.active .accordion-content {
    max-height: 200px;
    padding-block-end: var(--gutter);
    font-size: 1.3rem;
    text-align: left;
  }

  /* Media Queries for Mobile View */
  @media (max-width: 768px) {
    .accordion-header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      flex-wrap: nowrap;
      /* Evita que el contenido se desborde */
    }
    .accordion {
      padding-left: 10px;
      padding-right: 10px;
    }
    .accordion-content {
      display: flex;
      flex-direction: column;
      gap: 10px;
      max-height: 0;
      overflow: hidden;
      opacity: 0;
      padding: 0 15px;
      transition: max-height 0.5s ease-in-out, opacity 0.5s ease-in-out, padding 0.3s ease-in-out;
    }

    .accordion-item.active .accordion-content {
      max-height: 600px;
      /* Ajusta la altura suficiente */
      opacity: 1;
      padding: 15px;
    }

    .div_p {
      width: 100%;
      /* Asegura que los div_p ocupen todo el ancho disponible */
    }
  }

  .accordion-header-content {
    display: flex;
    align-items: center;
    flex-grow: 1;
    max-width: calc(100% - 40px);
    /* Resta el espacio de las flechas */
  }
  .accordion-icon {
    flex-shrink: 0;
    /* Evita que las flechas cambien de tama√±o */
    margin-left: 10px;
  }
  .subheading {
    padding: 0 10px;
  }
</style>
